{
  "agent": {
    "run_as_user": "root"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/app/log1.txt",
            "log_group_name": "log1.txt",
            "log_stream_name": "{instance_id}"
          },
          {
            "file_path": "/app/log2.txt",
            "log_group_name": "log2.txt",
            "log_stream_name": "{instance_id}"
          }
        ]
      }
    }
  }
}
